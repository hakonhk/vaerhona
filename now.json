{
  "name": "Værhøna",
  "scope": "hakonkrogh",
  "env": {
    "APP_PORT": "3000",
    "API_URI": "@vh-api-uri"
  },
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/p/veggli",
      "status": 301,
      "headers": { "Location": "https://værhøna.no/veggli" }
    },
    {
      "src": "/p/buvassbrenna",
      "status": 301,
      "headers": { "Location": "https://værhøna.no/buvassbrenna" }
    },
    {
      "src": "/",
      "dest": "/"
    },
    {
      "src": "^/([^/]+)$",
      "dest": "/place?placeName=$1"
    }
  ]
}
