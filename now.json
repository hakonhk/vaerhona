{
  "name": "Værhøna",
  "scope": "hakonkrogh",
  "env": {
    "APP_PORT": "3000",
    "API_URI": "@vh-api-uri"
  },
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/todo/veggli",
      "status": 301,
      "headers": { "Location": "https://værhøna.no/p/veggli" }
    },
    {
      "src": "/todo/buvassbrenna",
      "status": 301,
      "headers": { "Location": "https://værhøna.no/p/buvassbrenna" }
    },
    {
      "src": "/",
      "dest": "/"
    },
    {
      "src": "/(.*)",
      "dest": "/place/route-resolver"
    }
  ]
}
