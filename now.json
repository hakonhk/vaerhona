{
  "name": "Værhøna",
  "scope": "hakonkrogh",
  "env": {
    "MONGO_DB_URI": "@vh-mongo-db-uri",
    "VH_AWS_ACCESS_KEY_ID": "@vh-aws-access-key-id",
    "VH_AWS_SECRET_ACCESS_KEY": "@vh-aws-secret-access-key",
    "VH_AWS_REGION": "eu-west-1"
  },
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/p/veggli",
      "status": 301,
      "headers": { "Location": "https://værhøna.no/veggli" }
    },
    {
      "src": "/p/buvassbrenna",
      "status": 301,
      "headers": { "Location": "https://værhøna.no/buvassbrenna" }
    },
    {
      "src": "/",
      "dest": "/"
    },
    {
      "src": "^/([^/]+)$",
      "dest": "/place?placeName=$1"
    }
  ]
}
